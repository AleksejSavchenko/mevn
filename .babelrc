{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@": "./",
        "@components": "./client/components",
        "@pages": "./client/pages",
        "@store": "./client/store",
        "@utils": "./client/utils",
        "@config": "./server/config",
        "@routes": "./server/routes",
        "@controllers": "./server/controllers"
      }
    }]
  ]
}
